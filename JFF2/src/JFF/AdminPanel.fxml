<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="488.0" prefWidth="639.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JFF.AdminPanel">
   <children>
      <Text fill="#7760bc" layoutX="24.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="JFF FISHING SHACK">
         <font>
            <Font name="Arial Rounded MT Bold" size="34.0" />
         </font>
      </Text>
      <TableView fx:id="storeTable" layoutX="179.0" layoutY="91.0" prefHeight="167.0" prefWidth="282.0">
        <columns>
          <TableColumn fx:id="itemTable" prefWidth="103.0" text="item" />
          <TableColumn fx:id="codeTable" prefWidth="92.0" text="code" />
            <TableColumn fx:id="priceTable" prefWidth="86.0" text="price" />
        </columns>
      </TableView>
      <Button fx:id="viewStore" layoutX="273.0" layoutY="273.0" mnemonicParsing="false" onAction="#loadStore" text="VIEW STORE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button layoutX="38.0" layoutY="349.0" mnemonicParsing="false" onAction="#showUserInfo" prefHeight="39.0" prefWidth="168.0" text="Customer Details">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button layoutX="228.0" layoutY="349.0" mnemonicParsing="false" onAction="#showUserTransactions" prefHeight="39.0" prefWidth="168.0" text="Transactions">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button layoutX="421.0" layoutY="349.0" mnemonicParsing="false" onAction="#loadQuarrey" prefHeight="39.0" prefWidth="168.0" text="Quarees">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
